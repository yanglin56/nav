":24}},{"name":"url","generated":{"line":51,"column":35},"source":"main.js","original":{"line":67,"column":35}},{"generated":{"line":51,"column":38},"source":"main.js","original":{"line":67,"column":12}},{"generated":{"line":52,"column":0},"source":"main.js","original":{"line":68,"column":9}},{"generated":{"line":53,"column":0},"source":"main.js","original":{"line":69,"column":5}},{"generated":{"line":54,"column":0},"source":"main.js","original":{"line":70,"column":1}},{"generated":{"line":54,"column":1},"source":"main.js","original":{"line":63,"column":0}}],"sources":{"main.js":"const $siteList = $('.siteList')\nconst $lastLi = $siteList.find('li.last')\nconst x = localStorage.getItem('x')\nconst xObject = JSON.parse(x)\nconst hashMap = xObject || [\n    { logo: 'G', url: 'https://github.com/' },\n    { logo: 'J', url: 'https://jsbin.com/' }\n]\nconst simplifyUrl = (url) => {\n    return url.replace('https://', '')\n        .replace('http://', '')\n        .replace('www.', '')\n        .replace(/\\/.*/, '') // 删除 / 开头的内容\n        .replace('.com', '')\n        .replace('.cn', '')\n}\n\nconst render = () => {\n    $siteList.find('li:not(.last)').remove()\n    hashMap.forEach((node, index) => {\n        const $li = $(`<li>\n      <div class=\"site\">\n        <div class=\"logo\">${node.logo}</div>\n        <div class=\"link\">${simplifyUrl(node.url)}</div>\n        <div class=\"close\">\n          <svg class=\"icon\">\n            <use xlink:href=\"#icon-close\"></use>\n          </svg>\n        </div>\n      </div>\n    </li>`).insertBefore($lastLi)\n        $li.on('click', () => {\n            window.open(node.url)\n        })\n        $li.on('click', '.close', (e) => {\n            e.stopPropagation() // 阻止冒泡\n            hashMap.splice(index, 1)\n            render()\n        })\n    })\n}\n\nrender()\n\n$('.addButton').on('click', () => {\n    let url = window.prompt('请问你要添加的网址是啥？')\n    if (url.indexOf('http') !== 0) {\n        url = 'https://' + url\n    }\n    console.log(url)\n    hashMap.push({\n        logo: simplifyUrl(url)[0].toUpperCase(),\n        url: url\n    })\n    render()\n})\n\nwindow.onbeforeunload = () => {\n    const string = JSON.stringify(hashMap)\n    localStorage.setItem('x', string)\n}\n\n$(document).on('keypress', (e) => {\n    const { key } = e\n    for (let i = 0; i < hashMap.length; i++) {\n        if (hashMap[i].logo.toLowerCase() === key) {\n            window.open(hashMap[i].url)\n        }\n    }\n})"},"lineCount":null}},"hash":"afaf83dcef9f6522bf0f7ba7deb7cce1","cacheData":{"env":{}}}